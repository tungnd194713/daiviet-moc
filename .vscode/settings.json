{
    "liveSassCompile.settings.formats": [
        {
          "format": "compressed",
          "extensionName": ".css",
          "savePath": "~/../css"
        }
      ],
      // "liveSassCompile.settings.autoprefix": true,
      "liveSassCompile.settings.autoprefix": ["> 1%", "last 2 versions"],
      "liveSassCompile.settings.excludeList": [
        "/node_modules/",
        "/node_modules/",
        "/vendor/",
        ".vscode/",
        "/app/",
        "/bootstrap/",
        "/resources/",
        "/libs/"
      ],
}